<!DOCTYPE idea-plugin PUBLIC "Plugin/DTD"
        "http://plugins.intellij.net/plugin.dtd">
<idea-plugin>
    <name>API Comparator</name>
    <description>Plugin for viewing API changes in jar files.</description>

    <version>${project.version}</version>
    <!--<version>0.4</version>-->
    <vendor>Alexey Efimov</vendor>
    <idea-version since-build="7065"/>

    <application-components>
        <component>
            <implementation-class>org.intellij.apiComparator.ComparatorManager</implementation-class>
        </component>
        <component>
            <implementation-class>org.intellij.apiComparator.ComparatorConfiguration</implementation-class>
        </component>
    </application-components>
</idea-plugin>

